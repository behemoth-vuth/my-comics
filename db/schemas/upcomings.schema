create_table "upcomings", force: :cascade, options: "ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_bin" do |t|
  t.date :date, null: false
  t.integer :comic_id, null: false
  t.integer :volume
  t.integer :combo
  t.boolean :official
  t.datetime :grabbed_at
  t.datetime :skipped_at

  t.timestamps
end
